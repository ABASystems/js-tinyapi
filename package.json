    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.6.5",
    "clean-webpack-plugin": "^0.1.19",
    "code": "^5.2.0",
    "form-data": "^2.3.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "mocha": "^3.2.0",
    "peer-deps-externals-webpack-plugin": "^1.0.2",
